{
  "compilerOptions": {
    "skipLibCheck": true,
    "lib": ["esnext", "dom", "dom.iterable"],
    "esModuleInterop": true,
    "outDir": "./build/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": false
  },
  "include": ["src"]
}